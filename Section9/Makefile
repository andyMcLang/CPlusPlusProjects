.PHONY: clean All

All:
	@echo "----------Building project:[ CodingExercise17 - Debug ]----------"
	@cd "CodingExercise17" && "$(MAKE)" -f  "CodingExercise17.mk"
clean:
	@echo "----------Cleaning project:[ CodingExercise17 - Debug ]----------"
	@cd "CodingExercise17" && "$(MAKE)" -f  "CodingExercise17.mk" clean
