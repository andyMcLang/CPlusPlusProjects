.PHONY: clean All

All:
	@echo "----------Building project:[ CodingExercise15 - Debug ]----------"
	@cd "CodingExercise15" && "$(MAKE)" -f  "CodingExercise15.mk"
clean:
	@echo "----------Cleaning project:[ CodingExercise15 - Debug ]----------"
	@cd "CodingExercise15" && "$(MAKE)" -f  "CodingExercise15.mk" clean
