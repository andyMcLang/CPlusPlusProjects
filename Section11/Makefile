.PHONY: clean All

All:
	@echo "----------Building project:[ CodingExercise21 - Debug ]----------"
	@cd "CodingExercise21" && "$(MAKE)" -f  "CodingExercise21.mk"
clean:
	@echo "----------Cleaning project:[ CodingExercise21 - Debug ]----------"
	@cd "CodingExercise21" && "$(MAKE)" -f  "CodingExercise21.mk" clean
